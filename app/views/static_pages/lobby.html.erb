<br>
<div class="row">
<div class="col-sm-6">
  <h2 class="text-center">All Users</h2>
  <div class="user-list box">
    <% @users.each do |user| %>
      <div class="box">
        <p>User: <%= user.username %></p>
        <a href="#" class="btn btn-primary">Play Game</a>
      </div>
    <% end %>
  </div>
</div>



<div class="col-sm-6">
  <h2 class="text-center">Available Games</h2>
  <div class="user-list box">
    <ul>
      <% @games.each do |game| %>
        <li><%= game.game_title %><br>
        <%= link_to "Join!", game_path(game) %></li>
      <% end %>
    </ul>
  </div>
</div>

</div>
